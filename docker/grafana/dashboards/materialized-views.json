      },
      "pluginVersion": "10.3.1",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (eventType) (rate(eventsourcing_pipeline_views_updated_total[$__rate_interval]))",
          "legendFormat": "{{eventType}}",
          "refId": "A"
        }
      ],
