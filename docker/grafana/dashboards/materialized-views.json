        "legend": { "calcs": ["mean", "max"], "displayMode": "table", "placement": "bottom" },
        "tooltip": { "mode": "multi", "sort": "desc" }
      },
      "pluginVersion": "10.3.1",
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
          "expr": "sum by (application) (rate(eventsourcing_pipeline_events_received_total[$__rate_interval])) - sum by (application) (rate(eventsourcing_pipeline_events_processed_total[$__rate_interval]))",
          "legendFormat": "{{application}} backlog rate",
          "refId": "A"
        }
      ],
      "title": "Event Processing Backlog (Received - Processed Rate)",
      "description": "Difference between event receive rate and processing rate. Positive values indicate the view is falling behind; the pipeline is not keeping up with incoming events."
    },
    {
      "collapsed": false,
